{
  "name": "ebnis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -e dev gatsby develop --port 3023",
    "e2e": "yarn remove-folders && env-cmd -e e2e gatsby develop --port 3022",
    "build": "yarn remove-folders && cd ./plugins/offline-plugin && yarn prepare && cd ../../ && gatsby build",
    "build:p": "yarn env-cmd -e prod yarn build",
    "build:e2e": "yarn env-cmd -e e2e yarn build",
    "build:local:staging": "yarn env-cmd -e local-staging yarn build",
    "build-serve:e2e": "yarn build:e2e && yarn serve-local",
    "build:serve:local-staging": "yarn build:local:staging && yarn serve-local",
    "test": "env-cmd -e test yarn jest --runInBand ",
    "test:w": "yarn test --watch",
    "test:wc": "rimraf coverage && yarn test --watch --coverage",
    "test:c": "rimraf coverage && yarn test --coverage",
    "remove-folders": "rimraf public && rimraf .cache",
    "gen-gatsby-types": "apollo codegen:generate --endpoint=http://localhost:3022/___graphql --tagName=graphql --target=typescript --includes=src/components/**/*.ts* --outputFlat=src/graphql/gatsby-types",
    "gen-apollo-types-e": "env-cmd -e e2e node fetch-graphql-types.js",
    "gen-apollo-types-d": "env-cmd -e dev node fetch-graphql-types.js",
    "serve-local": "gatsby serve -p 3022",
    "build-deploy-staging": "yarn build:p && yarn deploy-staging",
    "build-deploy-prod": "yarn build:p && yarn deploy-prod",
    "deploy-staging": "mv .netlify-staging .netlify && yarn deploy && mv .netlify .netlify-staging",
    "deploy-prod": "mv .netlify-prod .netlify && yarn deploy && mv .netlify .netlify-prod",
    "deploy": "netlify deploy -p -d public",
    "cy-open": "env-cmd -e e2e cypress open",
    "cy-run-task": "env-cmd -e e2e cypress run",
    "cy-run": "start-server-and-test serve-local :3022 cy-run-task",
    "build-cy-run": "start-server-and-test build-serve:e2e :3022 cy-run-task",
    "type-check": "yarn tsc --noEmit --project .",
    "type-check-cypress": "yarn tsc --noEmit --project ./cypress",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
